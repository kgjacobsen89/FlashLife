<h1><%= @event.name %></h1>
<h2><%= @event.date %></h1>

<h3>Gallery</h3>
<ul>
<% unless @event.avatar.nil? %>
  <% @event.avatar.each do |a| %>
  <li>
    <%= image_tag event.a.url %>
  </li>
    <% end %>
<% end %>
</ul>

<%= link_to 'Back', user_path(@user) %>
<%= link_to 'Edit', edit_event_path(@event) %>
