<div class = "event-show">
<h1><%= @event.name %></h1>
<h2><%= @event.date %></h1>

<h3>Photo Gallery</h3>
<ul>
<% if @event.name == k["Title"] %>
	@image_urls[]
  <li>
    <%= image_tag %>
  </li>
<% end %>
</ul>

<%= link_to 'Back', user_path(@user) %>
<%= link_to 'Edit', edit_event_path(@event) %>
</div>